{
  "name": "sankey-hand-layout",
  "version": "0.2.1",
  "description": "A user-driven layout Sankey diagram library with drag, rotate, and resize interactions",
  "type": "module",
  "main": "./dist/sankey-hand-layout.umd.cjs",
  "module": "./dist/sankey-hand-layout.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/sankey-hand-layout.js"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/sankey-hand-layout.umd.cjs"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly --declaration --outDir dist",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "sankey",
    "diagram",
    "visualization",
    "svg",
    "interactive",
    "observable",
    "observable-framework",
    "hand-layout"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "dependencies": {
    "d3-interpolate": "^3.0.1"
  },
  "devDependencies": {
    "@types/d3-interpolate": "^3.0.4",
    "typescript": "^5.9.3",
    "vite": "^7.2.4"
  }
}
